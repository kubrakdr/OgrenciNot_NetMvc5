@using OgrenciNot_NetMvc5.Models.EntitiyFramework
@model List<TBL_KULUPLER>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Kulup Id</th>
        <th>Kulup Adı</th>
        <th>İŞLEMLER</th>
    </tr>
    @foreach (var klp in Model)
    {
        <tr>
            <td>@klp.KULUPID</td>
            <td>@klp.KULUPAD</td>
            <td><a href="/Kulupler/Sil/@klp.KULUPID" class="btn btn-danger">Sil</a>
            <a href="/Kulupler/KulupGetir/@klp.KULUPID" class="btn btn-success">Güncelle</a> </td>
        </tr>
    }
</table>

<a href="/Kulupler/YeniKulup" class="btn btn-primary">Yeni Kulüp Ekle</a>