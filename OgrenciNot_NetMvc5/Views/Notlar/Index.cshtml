@using OgrenciNot_NetMvc5.Models.EntitiyFramework
@model List<TBL_NOTLAR>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">

    <tr>
        <th>DERS ID</th>
        <th>OGR ID</th>
        <th>SINAV1</th>
        <th>SINAV2</th>
        <th>SINAV3</th>
        <th>PROJE</th>
        <th>ORTALAMA</th>
        <th>DURUM</th>
    </tr>
    @foreach (var not in Model)
    {
<tr>
    <td>@not.DERSID</td>
    <td>@not.OGRID</td>
    <td>@not.SINAV1</td>
    <td>@not.SINAV2</td>
    <td>@not.SINAV3</td>
    <td>@not.PROJE</td>
    <td>@not.ORTALAMA</td>
    <td>@not.DURUM</td>
</tr>
    }
    </table>


